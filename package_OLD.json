{
  "name": "@flex/framework",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install && mkdir -p ./mongo-data",
    "test": "STAGE=test jest",
    "test:coverage": "STAGE=test jest --coverage",
    "precommit": "lint-staged",
    "dev": "sls offline",
    "dev:full": "sls offline --runScheduler",
    "dev:server": "yarn start-db && yarn dev",
    "db:start": "mongod --dbpath ./mongo-data >> /dev/null &",
    "bump-version": "lerna changed --json | jq -r '.[] | .name' | xargs -I% sh -c 'cd packages/% && npm version patch && git add . && git commit -m \"chore(release): bump version to %s\"'",
    "flex": "npx babel-node ./packages/cli"
  },
  "prettier": "prettier-config-standard",
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*": "prettier --ignore-unknown -w",
    "*.js": "eslint --color --fix"
  },
  "repository": "git+https://github.com/Clark-s-Software-Consulting/flex-framework.git",
  "keywords": [
    "media",
    "s3",
    "lambda"
  ],
  "author": "Michael W. Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Clark-s-Software-Consulting/flex-framework/issues"
  },
  "homepage": "https://github.com/Clark-s-Software-Consulting/flex-framework#readme",
  "workspaces": [
    "packages/*",
    "libs/*",
    "services/*"
  ],
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@faker-js/faker": "7.6.0",
    "@types/node": "18.11.14",
    "babel-jest": "29.3.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "js-yaml": "4.1.0",
    "lerna": "6.1.0",
    "lint-staged": "13.1.0",
    "mongodb-memory-server": "8.10.2",
    "pre-commit": "1.2.2",
    "prettier": "2.8.1",
    "prettier-config-standard": "5.0.0",
    "serverless": "3.25.1",
    "serverless-add-api-key": "4.2.1",
    "serverless-bundle": "5.5.0",
    "serverless-offline": "12.0.2",
    "serverless-offline-aws-eventbridge": "2.0.3",
    "serverless-plugin-datadog": "5.14.1"
  },
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.19.6",
    "aws-sdk": "2.1273.0",
    "axios": "1.2.0",
    "commander": "^10.0.0",
    "date-fns": "2.29.3",
    "mongoose": "6.8.0",
    "oauth-1.0a": "2.2.6",
    "openai": "^3.1.0"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "main": "index.js"
}
